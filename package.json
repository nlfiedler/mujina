{
  "name": "mujina",
  "productName": "Mujina",
  "copyright": "Copyright Â© 2019 Nathan Fiedler",
  "version": "1.0.0",
  "repository": "https://github.com/nlfiedler/mujina.git",
  "description": "Electron-based interface for tanuki",
  "scripts": {
    "prestart": "gulp",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "pretest": "gulp",
    "test": "NODE_ENV=test electron-mocha --color 'test/**/*.js'",
    "lick": "license-checker --production --exclude MIT,ISC,LGPL,WTFPL,Apache-2.0,BSD-3-Clause,BSD-2-Clause,MPL-2.0,Unlicense",
    "pack": "gulp && electron-builder --dir",
    "dist": "gulp && electron-builder"
  },
  "keywords": [
    "photos",
    "videos",
    "assets",
    "digital",
    "management"
  ],
  "author": {
    "name": "Nathan Fiedler",
    "email": "nathanfiedler@gmail.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "main": "app/main.js",
  "build": {
    "appId": "com.example.mujina",
    "directories": {
      "app": "."
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": "pkg",
      "files": [
        "!node_modules/ffmpeg-static/bin/linux${/*}",
        "!node_modules/ffmpeg-static/bin/win${/*}",
        "!node_modules/ffprobe-static/bin/linux${/*}",
        "!node_modules/ffprobe-static/bin/win${/*}"
      ]
    }
  },
  "dependencies": {
    "bloomer": "^0.6.5",
    "date-fns": "^2.0.1",
    "electron-store": "^3.3.0",
    "electron-window-state": "^5.0.3",
    "ffmpeg-static": "^2.6.0",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^2.5.1",
    "history": "^4.7.2",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-lazyload-fadein": "^1.1.0",
    "react-redux": "^7.1.1",
    "react-redux-form": "^1.16.14",
    "react-resize-detector": "^4.2.0",
    "react-router": "^5.0.1",
    "react-visibility-sensor": "^5.1.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.5",
    "request": "^2.88.0",
    "sharp": "^0.22.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "del": "^5.1.0",
    "electron": "^5.0.10",
    "electron-builder": "^20.44.4",
    "electron-mocha": "^8.1.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^7.0.1",
    "license-checker": "^25.0.1",
    "mocha": "^6.2.0",
    "nock": "^11.3.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.3"
  }
}
